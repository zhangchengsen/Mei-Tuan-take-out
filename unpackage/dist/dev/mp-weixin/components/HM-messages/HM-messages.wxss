
.HMmessages.status{padding-top: 25px;}
.HMmessages.transparent{padding-top: calc(25px + 88rpx);}
.HMmessages.transparentH5{padding-top: 44px;}
@font-face {font-family:"HMfont";src:url('data:application/x-font-woff;charset=utf-8;base64,');}
.HMmessages .icon {font-family:"HMfont" !important;font-size:50rpx;font-style:normal;}
.HMmessages .icon.remind:before {content:"\e719";}
.HMmessages .icon.error:before {content:"\e71b";}
.HMmessages .icon.danger:before {content:"\e725";}
.HMmessages .icon.success:before {content:"\e727";}
.HMmessages .icon.disable:before {content:"\e734";}
.HMmessages .icon.help:before {content:"\e73a";}
.HMmessages .icon.close:before {content:"\e731";}
.HMmessages {width:100%;height:80rpx;position:fixed;top:-80rpx;left:0;z-index:997;display:-webkit-box;display:-webkit-flex;display:flex;-webkit-box-pack:start;-webkit-justify-content:flex-start;justify-content:flex-start;-webkit-box-align:center;-webkit-align-items:center;align-items:center;}
.HMmessages.show {-webkit-animation:show 0.3s linear both;animation:show 0.3s linear both;}
.HMmessages.hide {-webkit-animation:hide 0.3s linear both;animation:hide 0.3s linear both;}
@-webkit-keyframes show {0% {-webkit-transform:translateY(0);transform:translateY(0);opacity:0;}100% {-webkit-transform:translateY(80rpx);transform:translateY(80rpx);opacity:1;}}
@keyframes show {0% {-webkit-transform:translateY(0);transform:translateY(0);opacity:0;}100% {-webkit-transform:translateY(80rpx);transform:translateY(80rpx);opacity:1;}}
@-webkit-keyframes hide {0% {-webkit-transform:translateY(80rpx);transform:translateY(80rpx);opacity:1;}100% {-webkit-transform:translateY(0);transform:translateY(0);opacity:0;}}
@keyframes hide {0% {-webkit-transform:translateY(80rpx);transform:translateY(80rpx);opacity:1;}100% {-webkit-transform:translateY(0);transform:translateY(0);opacity:0;}}
.HMmessages .ico {width:80rpx;height:80rpx;display:-webkit-box;display:-webkit-flex;display:flex;-webkit-box-pack:center;-webkit-justify-content:center;justify-content:center;-webkit-box-align:center;-webkit-align-items:center;align-items:center;margin-left:2%;margin-right:-2%;}
.HMmessages .content {width:100%;height:80rpx;padding:0 2%;font-size:30rpx;white-space:nowrap;display:-webkit-box;display:-webkit-flex;display:flex;-webkit-box-align:center;-webkit-align-items:center;align-items:center;overflow:hidden;}
.HMmessages .closeBtn {width:60rpx;height:80rpx;display:-webkit-box;display:-webkit-flex;display:flex;-webkit-box-pack:center;-webkit-justify-content:center;justify-content:center;-webkit-box-align:center;-webkit-align-items:center;align-items:center;margin-right:2%;}
.HMmessages .closeBtn .close {font-family:"HMfont" !important;font-size:30rpx;font-style:normal;}


